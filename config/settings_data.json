  "platform_customizations": {
    "custom_css": [
      "\/* Стили для центрирования таблицы размеров *\/.size-chart {width: 100%; max-width: 600px; \/* Максимальная ширина таблицы, можно настроить *\/ margin: 0 auto; \/* Автоматические отступы для центрирования *\/ text-align: center;}",
      ".size-chart table {width: 100%; \/* Занимает всю ширину контейнера *\/ margin: 0 auto; \/* Центрирование таблицы *\/ border-collapse: collapse; \/* Удаление промежутков между ячейками *\/}",
      ".size-chart th,.size-chart td {text-align: center; \/* Центрирование текста внутри ячеек *\/ padding: 1px; \/* Отступы в ячейках *\/ border: 1px solid #ddd; \/* Границы ячеек *\/}",
      ".size-chart th {background-color: #f4f4f4; \/* Цвет фона заголовков *\/}",
      ".masonry-grid {display: flex; flex-wrap: wrap; justify-content: center; margin-top: 0; margin-bottom: 0; \/* Убраны отступы сверху и снизу *\/}",
      ".masonry-item {flex-basis: calc(33.3% - 4px); \/* или 44% для .masonry-item-large *\/ margin: 2px; box-sizing: border-box; position: relative;}",
      ".masonry-item-large {flex-basis: calc(50% - 4px); \/* Увеличиваем ширину на 20% *\/}",
      ".masonry-item img,.masonry-item-large img {max-width: 100%; height: auto; display: block; -webkit-filter: brightness(75%);}",
      ".centered-text {position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); color: white; \/* Выберите цвет текста *\/ font-size: 22px; \/* Размер шрифта *\/ font-weight: bold; \/* Делает текст жирным *\/}"
    ]
  }
}